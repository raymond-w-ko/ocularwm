compositor "BarrelDistortion" {
    technique {
        texture rt0 target_width target_height PF_A8R8G8B8

        // render scene to a texture
        target rt0 {
            input previous
        }

        target_output {
            // Start with clear output
            input none

            pass render_quad {
                material OcularWM/Compositor/BarrelWarp
                input 0 rt0
            }
        }
    }
}
